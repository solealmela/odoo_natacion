<odoo>
  <record model="ir.ui.view" id="natacion.session_tree_view">
    <field name="name">natacion.session.tree</field>
    <field name="model">natacion.session</field>
    <field name="arch" type="xml">
      <list>
        <field name="date"/>
        <field name="championship_id"/>
        <field name="registered_swimmers"/>
      </list>
    </field>
  </record>

  <record model="ir.ui.view" id="natacion.session_form_view">
    <field name="name">natacion.session.form</field>
    <field name="model">natacion.session</field>
    <field name="arch" type="xml">
      <form>
        <sheet>
          <group>
            <field name="date"/>
            <field name="championship_id"/>
            <field name="test_ids"/>
            <field name="registered_swimmers"/>
          </group>
        </sheet>
      </form>
    </field>
  </record>
</odoo>