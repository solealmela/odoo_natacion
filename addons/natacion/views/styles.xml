<odoo>
  <record model="ir.ui.view" id="natacion.style_tree_view">
    <field name="name">natacion.style.tree</field>
    <field name="model">natacion.style</field>
    <field name="arch" type="xml">
      <list>
        <field name="name"/>
        <field name="best_swimmers"/>
      </list>
    </field>
  </record>

  <record model="ir.ui.view" id="natacion.style_form_view">
    <field name="name">natacion.style.form</field>
    <field name="model">natacion.style</field>
    <field name="arch" type="xml">
      <form>
        <sheet>
          <group>
            <field name="name"/>
            <field name="best_swimmers"/>
          </group>
        </sheet>
      </form>
    </field>
  </record>
</odoo>